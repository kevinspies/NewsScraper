<div class="container-fluid">
    <!-- Content here -->
    <div class="row">
        <h1>Scrape either left winged, right winged, or neutral news sources!</h1>
    </div>
    <div class="row">
        <div class="col-sm">
            <button type="button" class="btn btn-outline-primary" id="cnnButton">CNN</button>
        </div>
        <div class="col-sm">
            <button type="button" class="btn btn-outline-primary" id="foxButton">FOX</button>
        </div>
        <div class="col-sm">
            <button type="button" class="btn btn-outline-primary" id="nytButton">NYT</button>
        </div>

    </div>

    <div class="row">
        <h1>Scraped news articles:</h1>
    </div>
    <div class="row" id="articles">
        <div class="col-sm" id="cnnArticles">
            {{!-- a response will give back the objects to fill this each call! --}}
            {{#each cnnArticle}}
            {{/each}}
        </div>
        <div class="col-sm" id="foxArticles">
            {{#each foxArticle}}
            {{/each}}
        </div>
        <div class="col-sm" id="nytArticles">
            {{#each nytArticle}}
            {{/each}}
        </div>
    </div>
</div>